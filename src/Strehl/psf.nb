Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"FFTShift", "[", "list_", "]"}], ":=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"n", ",", "m"}], "}"}], ",", "\[IndentingNewLine]", 
